{
  "requireConfig": true,
  "enable": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "semi": true,
  "useTabs": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "arrowParens": "always",
  "jsxBracketSameLine": true,
  "printWidth": 100,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "endOfLine": "lf",
  "bracketSpacing": true
}
